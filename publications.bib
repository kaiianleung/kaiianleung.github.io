	note = {Place: United States},
	pages = {1--13},
	file = {Full Text:C\:\\Users\\lkell\\Zotero\\storage\\UDUANJYQ\\Marrs Fuchsel - 2023 - Immigrant Latinas' Experiences with Intimate Partn.pdf:application/pdf},
