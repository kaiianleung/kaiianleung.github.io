}

@article{gwede_ck_exploring_2010,
	title = {Exploring {Disparities} and {Variability} in {Perceptions} and {Self}-{Reported} {Colorectal} {Cancer} {Screening} {Among} {Three} {Ethnic} {Subgroups} of {U}. {S}. {Blacks}.},
