	author = {{Collinge W.} and {Kahn J.} and {Walton T.} and {Kozak L.} and {Bauer-Wu S.} and {Fletcher K.} and {Yarnold P.} and {Soltysik R.}},
	year = {2013},
	note = {Place: Germany
Publisher: Springer Verlag (Tiergartenstrasse 17, Heidelberg D-69121, Germany)},
	pages = {1405--1414},
	file = {Full Text:C\:\\Users\\lkell\\Zotero\\storage\\BZ6UAVIC\\Collinge W. et al. - 2013 - Touch, Caring, and Cancer Randomized controlled t.pdf:application/pdf},
